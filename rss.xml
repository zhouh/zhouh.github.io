---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Hao Zhou's Blog</title>
    <link>{{ site.url }}{{ site.baseurl }}</link>
    <atom:link href="{{ site.url }}{{ site.baseurl }}/rss.xml" rel="self" type="application/rss+xml" />
    <description>Hao Zhou's Research Blog - Generative AI for discrete symbols</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>
 
    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>{{ site.url }}{{ site.baseurl }}{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Hao Zhou</author>
      <guid>{{ site.url }}{{ site.baseurl }}{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
 
  </channel>
</rss>
